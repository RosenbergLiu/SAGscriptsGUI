<Window x:Class="ASKOmaster.DBLookUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASKOmaster"
        mc:Ignorable="d"
        Title="DBLookUp" Height="450" Width="300">
    <Grid>
        <TextBox x:Name="LookupText" HorizontalAlignment="Center" Margin="0,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Button x:Name="btnLookUp" Content="Lookup" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Width="120" Height="36" FontSize="16" Click="btnLookUp_Click" IsDefault="True"/>
        <TextBlock x:Name="ResultBlock" HorizontalAlignment="Center" Margin="0,304,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="68" Width="47"/>

    </Grid>
</Window>
