<Window x:Class="ASKOmaster.Daily"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASKOmaster"
        mc:Ignorable="d"
        Title="Daily" Height="450" Width="800">
    <Grid>
        <ProgressBar x:Name="PgBar" HorizontalAlignment="Center" Height="18" Margin="0,383,0,0" VerticalAlignment="Top" Width="705" LargeChange="0.1"/>
        <Button x:Name="btnImport" Content="Import to DB" HorizontalAlignment="Left" Margin="295,174,0,0" VerticalAlignment="Top" Height="69" Width="171" Click="btnImport_Click" FontSize="20"/>
        <Button x:Name="btnGet" Content="GetCSV" HorizontalAlignment="Left" Margin="295,70,0,0" VerticalAlignment="Top" Height="69" Width="171" Click="GetCSV_Click" FontSize="20"/>
        <Button x:Name="btnUpdate" Content="Update Jobs" HorizontalAlignment="Left" Margin="295,314,0,0" VerticalAlignment="Top" Height="36" Width="171" Click="btnUpdate_Click"/>
        <Button x:Name="test" Content="Button" HorizontalAlignment="Left" Margin="153,126,0,0" VerticalAlignment="Top" Height="117" Width="118" Click="test_Click"/>

    </Grid>
</Window>
