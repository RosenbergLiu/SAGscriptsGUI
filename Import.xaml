<Window x:Class="ASKOmaster.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASKOmaster"
        mc:Ignorable="d"
        Title="Import" Height="400" Width="400">
    <Grid>
        <ComboBox x:Name="dbSelector" HorizontalAlignment="Center" Margin="0,62,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SelectCSV" Content="Select CSV" HorizontalAlignment="Center" Margin="0,120,0,0" VerticalAlignment="Top" Height="36" Width="144" FontSize="16" Click="SelectCSV_Click"/>
        <TextBlock x:Name="CSVpath" HorizontalAlignment="Center" Margin="0,192,0,0" TextWrapping="Wrap" Text="CSV Path" VerticalAlignment="Top"/>
        <Button x:Name="btnRun" Content="Import" HorizontalAlignment="Left" Margin="128,264,0,0" VerticalAlignment="Top" Height="59" Width="141" FontSize="20" Click="btnRun_Click"/>

    </Grid>
</Window>
